{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ustudy-ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
		"app/plugins/bootstrap/dist/css/bootstrap.css",
		"../node_modules/@swimlane/ngx-datatable/release/index.css",
		"../node_modules/@swimlane/ngx-datatable/release/themes/material.css",
		"../node_modules/@swimlane/ngx-datatable/release/assets/icons.css",
		"styles.css",
		"app/css/normalize.min.css",
		"app/plugins/iconfont/css/iconfont.css",
		"app/plugins/bootstrap/dist/css/bootstrap-theme.min.css",
		"app/plugins/bootstrap/dist/css/style.css",
		"app/plugins/bootstrapvalidator-master/dist/css/bootstrapValidator.min.css",
    "app/plugins/bootstrap-select/dist/css/bootstrap-select.css"
      ],
      "scripts": [
		"app/plugins/jquery.min.js",
		"app/plugins/bootstrap/dist/js/bootstrap.min.js",
		"app/plugins/bootstrapvalidator-master/dist/js/bootstrapValidator.min.js",
		"app/plugins/jquery.common.js",
    "app/plugins/bootstrap-select/dist/js/bootstrap-select.min.js"
	  ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
